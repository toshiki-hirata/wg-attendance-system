{
  "name": "vite-vue-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently  \"npm run mock-server\" \"vite\"",
    "mock-server": "cd ./mock-server && npm i && npm run dev",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "marp:pptx": "for file in hands_on_docs/slides/*.md; do npx marp \"$file\" --pptx -o \"hands_on_docs/exports/$(basename \"$file\" .md).pptx\"; done",
    "marp:pdf": "for file in hands_on_docs/slides/*.md; do npx marp \"$file\" --pdf -o \"hands_on_docs/exports/$(basename \"$file\" .md).pdf\"; done",
    "marp:html": "for file in hands_on_docs/slides/*.md; do npx marp \"$file\" --html -o \"hands_on_docs/exports/$(basename \"$file\" .md).html\"; done"
  },
  "dependencies": {
    "axios": "1.8.4",
    "pinia": "3.0.1",
    "pinia-plugin-persistedstate": "4.2.0",
    "vue": "3.5.13",
    "vue-router": "4.5.0",
    "vee-validate": "4.13.2",
    "@vee-validate/rules": "4.13.2"
  },
  "devDependencies": {
    "@eslint/js": "9.24.0",
    "@marp-team/marp-cli": "^4.1.2",
    "@vitejs/plugin-vue": "5.2.2",
    "@vue/tsconfig": "0.7.0",
    "autoprefixer": "10.4.21",
    "concurrently": "9.1.2",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-vue": "10.0.0",
    "globals": "16.0.0",
    "lefthook": "1.11.10",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "tailwindcss": "3.4.17",
    "typescript": "~5.7.2",
    "typescript-eslint": "8.30.1",
    "vite": "6.2.2",
    "vue-tsc": "2.2.8"
  },
  "packageManager": "pnpm@10.6.5+sha512.cdf928fca20832cd59ec53826492b7dc25dc524d4370b6b4adbf65803d32efaa6c1c88147c0ae4e8d579a6c9eec715757b50d4fa35eea179d868eada4ed043af"
}
